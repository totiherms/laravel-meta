{
    "name": "vkovic/laravel-meta",
    "description": "Laravel meta storage for different purposes",
    "keywords": [
        "laravel",
        "meta",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir KoviÄ‡",
            "email": "vlada.kovic@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Vkovic\\LaravelMeta\\": "src/package"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vkovic\\LaravelMeta\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "laravel/laravel": "^5.5|^6.0",
        "orchestra/testbench": "^3.5|^4.0",
        "orchestra/database": "^3.5|^4.0",
        "phpunit/phpunit": "^6.3|^7.0|^8.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vkovic\\LaravelMeta\\Providers\\LaravelMetaServiceProvider"
            ],
            "aliases": {
                "Meta": "Vkovic\\LaravelMeta\\Facades\\MetaFacade"
            }
        }
    }
}